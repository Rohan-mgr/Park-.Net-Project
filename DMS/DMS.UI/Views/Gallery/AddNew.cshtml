
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "AddNew";
}

@using (Html.BeginForm("SaveImage","Gallery",FormMethod.Post,new { @class="",@enctype="multipart/form-data"}))
{
<div class="row form-group">
    <label class="col-md-2">Select Photo</label>
    <div class="col-md-4">
        <input type="file" class="form-control" name="SelectedFile" />
    </div>
    <label class="col-md-2">Add Car Name</label>
    <div class="col-md-4">
        <input type="text" class="form-control" name="carname" id="carname" />
    </div>
</div>
    <div class="row form-group">
        <input type="submit" class="btn btn-success" value="Save"/>
    </div>
}

