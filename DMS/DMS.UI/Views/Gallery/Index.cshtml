@model List<DMS.DAL.DatabaseContext.gallery>


@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "Index";
    int sn = 0;
}


<table class="table table-bordered">
    <tr>
        <td>SN</td>
        <td>Uploaded File</td>
    </tr>
    @foreach (var data in Model)
    {
        var pic = (data.photo_path + "/" + data.photo_name);
        <tr>
            <td>@(++sn)</td>
            <td>
                <img src="@pic" height="200" width="200" />
            </td>
        </tr>

    }
</table>